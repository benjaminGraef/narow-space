    </key>

    <!-- ================================= -->
    <!-- Move focus to window in workspace -->
    <!-- ================================= -->
    <key name="narrow-space-move-focus-left" type="as">
      <default>["&lt;Super&gt;h"]</default>
      <summary>Move focus to window left</summary>
      <description>Move focused window using Super+h</description>
    </key>

    <key name="narrow-space-move-focus-right" type="as">
      <default>["&lt;Super&gt;l"]</default>
      <summary>Move focus to window right</summary>
      <description>Move focused window using Super+l</description>
    </key>

    <key name="narrow-space-move-focus-up" type="as">
      <default>["&lt;Super&gt;k"]</default>
      <summary>Move focus to window up</summary>
      <description>Move focused window using Super+k</description>
    </key>

    <key name="narrow-space-move-focus-down" type="as">
      <default>["&lt;Super&gt;j"]</default>
      <summary>Move focus to window down</summary>
      <description>Move focused window using Super+j</description>
    </key>

    <!-- =============================== -->
    <!-- Switch modes within a workspace -->
    <!-- =============================== -->
    <key name="narrow-space-change-workspace-mode" type="as">
      <default>["&lt;Super&gt;comma"]</default>
      <summary>Change the mode of the current workspace</summary>
      <description>Modes can be tile (vertical, horizontal) or stacking</description>
    </key>

    <!-- ============================== -->
    <!-- Resizing windows or workspaces -->
    <!-- ============================== -->
    <key name="narrow-space-resize-inc" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;p"]</default>
      <summary>Resize window +</summary>
      <description>Resize window with super shift + </description>
    </key>

    <key name="narrow-space-resize-dec" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;o"]</default>
      <summary>Resize window .</summary>
      <description>Resize window with super shift . </description>
    </key>

    <!-- ================================= -->
    <!-- Moving windows within a workspace -->
    <!-- ================================= -->
    <key name="narrow-space-move-window-left" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;h"]</default>
      <summary>Resize window +</summary>
      <description>Move active window one to the left within the workspace.
      Works only in vertical tiling mode</description>
    </key>

    <key name="narrow-space-move-window-right" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;l"]</default>
      <summary>Resize window +</summary>
      <description>Move active window one to the right within the workspace.
      Works only in vertical tiling mode</description>
    </key>

    <key name="narrow-space-move-window-up" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;k"]</default>
      <summary>Resize window +</summary>
      <description>Move active window one up within the workspace.
      Works only in vertical tiling mode</description>
    </key>

    <key name="narrow-space-move-window-down" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;j"]</default>
      <summary>Resize window +</summary>
      <description>Move active window one to down within the workspace.
      Works only in vertical tiling mode</description>
    </key>

    <!-- ============================================== -->
    <!-- Enter/Leave Service mode                       -->
    <!-- In service mode windows can be joined          -->
    <!-- to a workspace (workspace withing workspace)   -->
    <!-- by using the "narrow-space-move-window-" functions -->
    <!-- ======================== -->
    <key name="narrow-space-service-mode" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;x"]</default>
      <summary>Resize window +</summary>
      <description>Enter and leave service mode</description>
    </key>

    <!-- ============================================== -->
    <!-- Toggle currently active window floating/tiling -->
    <!-- ============================================== -->
    <key name="narrow-space-toggle-floating" type="as">
      <default>["&lt;Super&gt;&lt;Shift&gt;f"]</default>
      <summary>Resize window +</summary>
      <description>Disable/Enable again the tiling for the focused window </description>
    </key>

  </schema>
</schemalist>
